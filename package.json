  "devDependencies": {
    "@csstools/postcss-global-data": "3.0.0",
    "@eslint/js": "9.18.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.5.2",
    "@testing-library/vue": "8.1.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/eslint-plugin": "1.1.24",
