    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "3.0.0",
    "@eslint/js": "9.30.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.6.1",
    "@testing-library/vue": "8.1.0",
    "@vitejs/plugin-vue": "6.0.0",
    "@vitest/eslint-plugin": "1.3.3",
